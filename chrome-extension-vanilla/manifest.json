{
  "manifest_version": 3,
  "name": "Digital Bookmarking",
  "version": "1.0",
  "description": "A Chrome extension to digitally bookmark web pages.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup/popup.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
